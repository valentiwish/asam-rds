<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asam.human.mapper.HumUserOrgDao">
    <!-- 批量插入用户数据 -->
    <insert id="insertAll">
        INSERT INTO hum_user_org (sys_user_id, sys_org_id)
        VALUES
        <foreach collection="list" item="data" separator=",">
            (#{data.sysUserId}, #{data.sysOrgId})
        </foreach>
    </insert>
</mapper>