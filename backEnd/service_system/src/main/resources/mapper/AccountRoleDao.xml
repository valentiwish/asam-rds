<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asam.system.mapper.AccountRoleDao">
    <!-- 批量插入用户数据 -->
    <insert id="insertAll">
        INSERT INTO sys_account_role (sys_account_id,sys_role_id)
        VALUES
        <foreach collection="list" item="data" separator=",">
            (#{data.sysAccountId}, #{data.sysRoleId})
        </foreach>
    </insert>
</mapper>